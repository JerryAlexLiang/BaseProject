<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="120dp"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:layout_margin="8dp"
    android:background="@drawable/bg_solid_round_color_white_radius_def"
    android:orientation="vertical"
    android:paddingTop="8dp"
    android:paddingBottom="8dp">

    <!--style="?android:attr/borderlessButtonStyle"-->
    <TextView
        android:id="@+id/dialog_map_menu_share"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="分享"

        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_custom"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="自定义"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_offline_custom"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="离线地图"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_standard"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="标准地图"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_satellite"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="卫星地图"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_night"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="夜间模式"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_navigation"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="导航模式"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/dialog_map_menu_custom_overlay"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="自定义的Overlay"
        android:textColor="@color/text_invert"
        android:textSize="15sp" />

</LinearLayout>